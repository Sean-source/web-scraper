{{#each news}}

<div class="card" style="width: 18rem;">
  <img src="{{image}}" class="card-img-top" alt="...">
  <div class="card-body">
    <a href = "{{URL}}"><h5 class="card-title">{{headline}}</h5></a>
    <p class="card-text">{{summary}}</p>
    <a href="#" class="btn btn-primary">Save</a>
  </div>
</div>

{{/each}}